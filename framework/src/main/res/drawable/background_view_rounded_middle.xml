<?xml version="1.0" encoding="UTF-8"?>
<inset android:insetLeft="0.0px" android:insetRight="0.0px" android:insetTop="0.0px" android:insetBottom="0.0px"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <gradient android:startColor="@color/base_start_color_pressed" android:endColor="@color/base_end_color_pressed" android:angle="270.0" />
                <corners android:radius="0.0dip" />
            </shape>
        </item>
        
        <item  android:state_enabled="false">
            <shape>
                <gradient 
                    android:startColor="@color/base_start_color_disable" android:endColor="@color/base_end_color_disable" android:angle="270.0" />
                <corners android:radius="0.0dip" />
            </shape>
        </item>
        
        <item>
            <shape>
                <gradient android:startColor="@color/base_start_color_default" android:endColor="@color/base_end_color_default" android:angle="270.0" />
                <corners android:radius="0.0dip" />
            </shape>
        </item>
    </selector>
</inset>